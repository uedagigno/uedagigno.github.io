<?xml version="1.0" encoding="UTF-8"?>
<!--アプリ設定-->
<app_config>
    <!--アプリ情報-->
    <app_info>
        <!--アプリ正式名称（任意：クライアント側では必要としません）-->
        <name>フレッツ・あずけ～る</name>
        <!--iOS-->
        <ios>
            <!--iOS バージョン名（必須）-->
            <ios_version>5.0.0</ios_version>
            <!--iOS ビルド番号（必須：整数）-->
            <ios_build>34</ios_build>
            <!--iOS 強制アップデート（必須： 1(する) / 0(しない)）-->
            <ios_forced_update>1</ios_forced_update>
        </ios>
        <!--Android-->
        <android>
            <!--Android バージョン名（必須）-->
            <android_version_name>5.0.0</android_version_name>
            <!--Android バージョンコード（必須：整数）-->
            <android_version_code>100</android_version_code>
            <!--Android 強制アップデート（必須： 1(する) / 0(しない)）-->
            <android_forced_update>1</android_forced_update>
        </android>
    </app_info>
</app_config>
