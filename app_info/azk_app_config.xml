<?xml version="1.0" encoding="UTF-8"?>
<!--アプリ設定-->
<app_config>
    <!--アプリ情報-->
    <app_info>
        <!--アプリ正式名称（任意：クライアント側では必要としません）-->
        <name>フレッツ・あずけ～る</name>
        <!--iOS-->
        <ios>
            <!--iOS バージョン名（必須）-->
            <ios_version>7.1.0</ios_version>
            <!--iOS ビルド番号（必須：整数）-->
            <ios_build>50</ios_build>
            <!--iOS 強制アップデート（必須： 1(する) / 0(しない)）-->
            <ios_forced_update>1</ios_forced_update>
        </ios>
        <!--Android-->
        <android>
            <!--Android バージョン名（必須）-->
            <android_version_name>8.1.0</android_version_name>
            <!--Android バージョンコード（必須：整数）-->
            <android_version_code>1200</android_version_code>
            <!--Android 強制アップデート（必須： 1(する) / 0(しない)）-->
            <android_forced_update></android_forced_update>
        </android>
    </app_info>
    <!--外部リンクURL-->
    <external_url>
        <!--利用規約（必須：必ず存在するページのアドレスを入力してください）-->
        <terms>https://uedagigno.github.io/app_info/terms.html</terms>

        <!--無料プラン_プラン変更（任意：必要ない場合は空）-->
        <plan_change>https://ja.wikipedia.org/wiki/17002</plan_change>
        <!--有料プラン_プラン変更（任意：必要ない場合は空）-->
        <plan_change_pay>https://ja.wikipedia.org/wiki/17003</plan_change_pay>
        <!--PROプラン_プラン変更（任意：必要ない場合は空）-->
        <plan_change_pro>https://ja.wikipedia.org/wiki/17004</plan_change_pro>
        <!--Aioプラン_プラン変更（任意：必要ない場合は空）-->
        <plan_change_aio></plan_change_aio>
        <!--お知らせ（任意：必要ない場合は空）-->
        <notice>https://ja.wikipedia.org/wiki/17005</notice>
        <!--無料プラン_お問い合わせ（任意：必要ない場合は空）-->
        <contact>https://ja.wikipedia.org/wiki/17006</contact>
        <!--有料プラン_お問い合わせ（任意：必要ない場合は空）-->
        <contact_pay>https://ja.wikipedia.org/wiki/17007</contact_pay>
        <!--PROプラン_お問い合わせ（任意：必要ない場合は空）-->
        <contact_pro>https://ja.wikipedia.org/wiki/17008</contact_pro>
        <!--Aioプラン_お問い合わせ（任意：必要ない場合は空）-->
        <contact_aio>https://ja.wikipedia.org/wiki/17009</contact_aio>
        <!--秘密のヒント登録（任意：必要ない場合は空）-->
        <secret_hint_regist>https://ja.wikipedia.org/wiki/17010</secret_hint_regist>
        <!--プライバシーポリシー（任意：必要ない場合は空）-->
        <privacy_policy>https://ja.wikipedia.org/wiki/17011</privacy_policy>
    </external_url>
</app_config>
