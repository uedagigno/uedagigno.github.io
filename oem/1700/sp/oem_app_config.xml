<?xml version="1.0" encoding="UTF-8"?>
<!--アプリ設定-->
<app_config>
    <!--OEM情報-->
	<oem_info>
        <!--OEM_ID（必須：クライアント側で妥当性チェックに使用します）-->
		<oem_id>1700</oem_id>
        <!--事業者正式名称（任意：クライアント側では必要としません）-->
		<storage_sp_name>OEM事業者（１）</storage_sp_name>
	</oem_info>
    <!--アプリ情報-->
	<app_info>
        <!--アプリ正式名称（任意：クライアント側では必要としません）-->
		<app_name>OEMデモ1</app_name>
        <!--iOS--> 
		<ios> 
            <!--iOS バージョン名（必須）--> 
			<ios_version>1.0.0</ios_version>
            <!--iOS ビルド番号（必須：整数）--> 
			<ios_build>2</ios_build>
            <!--iOS 強制アップデート（必須： 1(する) / 0(しない)）-->
			<ios_forced_update>0</ios_forced_update>
            <!--iOS AppStore アプリURL（必須：必ず存在するページのアドレスを入力してください）--> 
			<ios_appstore_url>http://itunes.apple.com/WebObjects/MZStore.woa/wa/viewSoftware?id=680432407&amp;mt=8</ios_appstore_url>
		</ios>
        <!--Android--> 
		<android> 
            <!--Android バージョン名（必須）-->
			<android_version_name>1.0.0</android_version_name>
            <!--Android バージョンコード（必須：整数）-->
			<android_version_code>2</android_version_code>
            <!--Android 強制アップデート（必須： 1(する) / 0(しない)）-->
			<android_forced_update>０</android_forced_update>
            <!--Android GooglePlay アプリURL（必須：必ず存在するページのアドレスを入力してください）-->
			<android_googleplay_url>https://play.google.com/store/apps/details?id=com.flets.azukeru&amp;hl=ja</android_googleplay_url>
		</android>
	</app_info>
    <!--外部リンクURL-->
	<external_url>
        <!--利用規約（必須：必ず存在するページのアドレスを入力してください）-->
        <terms>https://ja.wikipedia.org/wiki/17000</terms>
        <!--注意事項（必須：必ず存在するページのアドレスを入力してください）-->
        <caution>https://ja.wikipedia.org/wiki/17001</caution>
        <!--プラン変更（任意：必要ない場合は空）-->
        <plan_change>https://ja.wikipedia.org/wiki/17002</plan_change>
        <!--お知らせ（任意：必要ない場合は空）-->
        <notice>https://ja.wikipedia.org/wiki/17003</notice>
        <!--お問い合わせ（任意：必要ない場合は空）-->
        <contact>https://ja.wikipedia.org/wiki/17004</contact>
        <!--秘密のヒント登録（任意：必要ない場合は空）-->
        <secret_hint_regist>https://ja.wikipedia.org/wiki/17005</secret_hint_regist>
        <!--本登録の案内URL（任意：必要ない場合は空）-->
        <formal_registration>https://ja.wikipedia.org/wiki/17006</formal_registration>
        <!--プライバシーポリシー（任意：必要ない場合は空）-->
        <privacy_policy>https://ja.wikipedia.org/wiki/17007</privacy_policy>
    </external_url>
</app_config>
