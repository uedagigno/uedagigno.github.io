        <plan_change>https://flets.com/azukeru/pro/flow.html</plan_change>
        <!--お知らせ（任意：必要ない場合は空）-->
        <notice>https://flets.com/s/azukeru/support.html</notice>
        <!--お問い合わせ（任意：必要ない場合は空）-->
        <contact>https://flets.com/s/azukeru/support.html</contact>
        <!--秘密のヒント登録（任意：必要ない場合は空）-->
        <secret_hint_regist>https://flets.com/azukeru/login/support.html</secret_hint_regist>
        <!--本登録の案内URL（任意：必要ない場合は空）-->
        <formal_registration>https://flets.com/azukeru/pro/settei.html</formal_registration>
